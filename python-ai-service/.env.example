# Python AI Service Environment Variables

# Environment
ENV=development

# Database (shared with Rails)
DATABASE_URL=postgresql://localhost/honeybee_development

# Rails API
RAILS_API_URL=http://localhost:3000
RAILS_API_TOKEN=development-key

# AI Service API Key (for Rails to authenticate)
AI_SERVICE_API_KEY=development-key

# OpenAI
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4

# Sentry (optional)
SENTRY_DSN=

# Logging
SQL_ECHO=false
LOG_LEVEL=INFO

# Redis (optional, for agent communication)
REDIS_URL=redis://localhost:6379/0
